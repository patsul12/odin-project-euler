function primeFactorize(number) {
  this.number = number;
  this.primeNumber = false;
  this.primeFactors = [];

  for(i=2; i<number/2; i++) {
    if (number % i === 0 && isPrime(i) {
      primeFactors[primeFactors.length] = i;
    }
  }

  var isPrime = function(number) {
    for(i=2; i<number/2; i++) {
      if (number % 1 === 0) {
        return false;
      }
    }
  }
  console.log(primeFactors[-1]);
}
